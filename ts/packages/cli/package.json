{
  "name": "@configu/cli",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "configu cli",
  "author": "Configu",
  "scripts": {
    "clean:build": "shx rm -rf build dist",
    "clean:cache": "shx rm -rf oclif.manifest.json package-lock.json tsconfig.tsbuildinfo tmp",
    "build:tsc": "tsc --build",
    "build:docker": "docker build --progress plain -t configu/cli -f ./Dockerfile .",
    "build:oclif": "oclif pack tarballs --parallel",
    "prebuild:oclif": "concurrently \"npm:clean:*\" && shx cp ../../package-lock.json ./package-lock.json && npm run build:tsc && oclif manifest && oclif readme --no-aliases",
    "postbuild:oclif": "npm run clean:cache",
    "test": "echo NO TESTS"
  },
  "oclif": {
    "bin": "configu",
    "dirname": "configu",
    "scope": "configu",
    "commands": "./build/commands",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-version"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "update": {
      "node": {
        "version": "16.17.1",
        "targets": [
          "linux-arm",
          "linux-x64",
          "darwin-arm64",
          "darwin-x64",
          "win32-x64"
        ]
      },
      "s3": {
        "bucket": "cli.configu.com",
        "folder": "",
        "acl": " ",
        "host": "https://cli.configu.com",
        "xz": false
      }
    }
  },
  "engines": {
    "node": "=16.17.1"
  },
  "exports": "./build/index.js",
  "types": "./build/index.d.ts",
  "bin": {
    "configu": "./bin/run"
  },
  "files": [
    "/bin",
    "/build/**/*.js",
    "/oclif.manifest.json"
  ],
  "homepage": "https://configu.com",
  "keywords": [
    "configu",
    "configu-cli"
  ],
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.185.0",
    "@azure/arm-appservice": "^13.0.2",
    "@azure/identity": "^3.0.0",
    "@configu/lib": "0.2.0",
    "@configu/node": "0.4.0",
    "@configu/ts": "0.4.0",
    "@oclif/core": "^1.16.4",
    "@oclif/plugin-autocomplete": "^1.3.1",
    "@oclif/plugin-help": "^5.1.14",
    "@oclif/plugin-not-found": "^2.3.3",
    "@oclif/plugin-plugins": "^2.1.1",
    "@oclif/plugin-update": "^3.0.2",
    "@oclif/plugin-version": "^1.1.3",
    "axios": "^0.27.2",
    "chalk": "4.1.2",
    "change-case": "^4.1.2",
    "ci-info": "^3.4.0",
    "dotenv": "^16.0.3",
    "inquirer": "8.2.4",
    "lodash": "^4.17.21",
    "openid-client": "^5.1.10"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.2",
    "@types/inquirer": "8.2.3",
    "@types/lodash": "^4.14.186",
    "aws-sdk": "^2.1230.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "globby": "11.0.4",
    "oclif": "^3.2.13",
    "tslib": "^2.4.0"
  }
}
