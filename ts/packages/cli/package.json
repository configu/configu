{
  "name": "@configu/cli",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "configu cli",
  "author": "Configu",
  "scripts": {
    "clean:build": "shx rm -rf build dist",
    "clean:cache": "shx rm -rf oclif.manifest.json package-lock.json tsconfig.tsbuildinfo tmp",
    "build:tsc": "tsc --build",
    "build:docker": "docker build --progress plain -t configu/cli -f ./Dockerfile .",
    "build:oclif": "oclif pack tarballs --parallel",
    "prebuild:oclif": "concurrently \"npm:clean:*\" && shx cp ../../package-lock.json ./package-lock.json && npm run build:tsc && oclif manifest && oclif readme --no-aliases",
    "postbuild:oclif": "npm run clean:cache",
    "test": "echo NO TESTS"
  },
  "oclif": {
    "bin": "configu",
    "dirname": "configu",
    "scope": "configu",
    "commands": "./build/commands",
    "topicSeparator": " ",
    "topics": {
      "store": {
        "description": "manage config-store connection as labels to later be used with the --store flag value in following commands"
      }
    },
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-version"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "update": {
      "node": {
        "version": "18.14.2",
        "targets": [
          "linux-arm",
          "linux-x64",
          "darwin-arm64",
          "darwin-x64",
          "win32-x64"
        ]
      },
      "s3": {
        "bucket": "cli.configu.com",
        "folder": "",
        "acl": " ",
        "host": "https://cli.configu.com",
        "xz": false
      }
    }
  },
  "engines": {
    "node": ">=v16.0.0"
  },
  "exports": "./build/index.js",
  "types": "./build/index.d.ts",
  "bin": {
    "configu": "./bin/run"
  },
  "files": [
    "/bin",
    "/build/**/*.js",
    "/oclif.manifest.json"
  ],
  "homepage": "https://configu.com",
  "keywords": [
    "configu",
    "configu-cli"
  ],
  "dependencies": {
    "@configu/lib": "^0.5.0-next.1",
    "@configu/node": "^0.10.0-next.29",
    "@configu/ts": "^0.10.0-next.29",
    "@oclif/core": "^2.4.0",
    "@oclif/plugin-autocomplete": "^2.1.4",
    "@oclif/plugin-help": "^5.2.7",
    "@oclif/plugin-not-found": "^2.3.21",
    "@oclif/plugin-plugins": "^2.3.2",
    "@oclif/plugin-update": "^3.1.6",
    "@oclif/plugin-version": "^1.2.1",
    "axios": "^1.3.4",
    "chalk": "4.1.2",
    "change-case": "^4.1.2",
    "ci-info": "^3.8.0",
    "dotenv": "^16.0.3",
    "fuzzy": "^0.1.3",
    "inquirer": "8.2.5",
    "inquirer-autocomplete-prompt": "2.0.0",
    "lodash": "^4.17.21",
    "log-symbols": "4.1.0",
    "open": "^8.4.2",
    "openid-client": "^5.4.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.9",
    "@types/inquirer": "8.2.5",
    "@types/inquirer-autocomplete-prompt": "3.0.0",
    "@types/lodash": "^4.14.191",
    "aws-sdk": "^2.1328.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "globby": "11.0.4",
    "oclif": "^3.7.0",
    "tslib": "^2.5.0"
  }
}
