    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@gquittet/graceful-server": "4.0.9",
    "@scalar/fastify-api-reference": "^1.24.39",
