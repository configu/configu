{
  "name": "@configu/sdk",
  "version": "0.0.0",
  "description": "Configu TypeScript SDK",
  "keywords": [
    "configu-sdk",
    "configu-ts",
    "configu-typescript",
    "sdk",
    "typescript",
    "typescript-sdk"
  ],
  "homepage": "https://configu.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/configu/configu",
    "directory": "packages/sdk"
  },
  "bugs": "https://github.com/configu/configu/issues",
  "license": "Apache-2.0",
  "author": {
    "name": "@configu/dev",
    "email": "dev@configu.com",
    "url": "https://configu.com"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./expressions": {
      "import": {
        "types": "./dist/expressions/index.d.ts",
        "default": "./dist/expressions/index.mjs"
      },
      "require": {
        "types": "./dist/expressions/index.d.cts",
        "default": "./dist/expressions/index.cjs"
      }
    },
    "./stores": {
      "import": {
        "types": "./dist/stores/index.d.ts",
        "default": "./dist/stores/index.mjs"
      },
      "require": {
        "types": "./dist/stores/index.d.cts",
        "default": "./dist/stores/index.cjs"
      }
    },
    "./commands": {
      "import": {
        "types": "./dist/commands/index.d.ts",
        "default": "./dist/commands/index.mjs"
      },
      "require": {
        "types": "./dist/commands/index.d.cts",
        "default": "./dist/commands/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "tsup"
  },
  "dependencies": {
    "@segment/ajv-human-errors": "^2.14.1",
    "@vitest/expect": "^2.1.8",
    "acorn": "^8.14.0",
    "acorn-walk": "^8.3.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chai": "^5.1.2",
    "chai-subset": "^1.6.0",
    "jest-extended": "^4.0.2",
    "json-schema-to-ts": "^3.1.1",
    "lodash": "^4.17.21",
    "ses": "^1.10.0",
    "validator": "^13.12.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/chai": "^5.0.1",
    "@types/chai-subset": "^1.3.5",
    "@types/validator": "^13.12.2",
    "type-fest": "^4.30.2"
  }
}
