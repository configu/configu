{
  "name": "@configu/cli",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "Configu CLI",
  "author": "@configu/dev",
  "homepage": "https://configu.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/configu/configu",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/configu/configu/issues",
    "email": "dev@configu.com"
  },
  "keywords": [
    "cfgu",
    "configu",
    "configu-cli",
    "configu-command-line"
  ],
  "type": "module",
  "bin": {
    "configu": "./dist/run.cjs"
  },
  "exports": {
    "./package.json": "./package.json"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "tsup",
    "compile": "tsx compile.mts",
    "build:docker": "docker build --build-arg NODE_VERSION=22.11.0 --build-arg CONFIGU_VERSION=1.0.0-next.226 --progress plain -t configu/cli -f ./Dockerfile .",
    "dev": "tsx src/run.ts",
    "start": "node dist/run.cjs",
    "configu": "~/.configu/bin/configu",
    "test:post-release": "tsx --test ./compiler/**/*.test.ts"
  },
  "dependencies": {
    "@configu/common": "workspace:*",
    "@configu/formatters": "workspace:*",
    "@configu/sdk": "workspace:*",
    "axios": "^1.7.4",
    "change-case": "4.1.2",
    "clipanion": "^4.0.0-rc.4",
    "consola": "^3.2.3",
    "dotenv": "16.4.5",
    "inquirer": "12.0.1",
    "lodash": "^4.17.21",
    "open": "10.1.0",
    "openid-client": "5.7.1",
    "std-env": "^3.8.0",
    "tty-table": "4.2.3",
    "typanion": "3.14.0",
    "tiged": "2.12.7"
  },
  "devDependencies": {},
  "bundleDependencies": []
}
