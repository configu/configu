  },
  "bugs": {
    "url": "https://github.com/configu/configu/issues",
    "email": "dev@configu.com"
  },
  "keywords": [
    "cfgu",
    "configu",
    "configu-common",
    "configu-lib",
    "configu-node"
  ],
  "type": "module",
  "types": "./src/index.ts",
